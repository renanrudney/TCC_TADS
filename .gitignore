# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# frontend

# dependencies
frontend/node_modules
frontend/.pnp
frontend/.pnp.js

# testing
frontend/coverage

# production
frontend/build

# misc
frontend/.DS_Store
frontend/.env.local
frontend/.env.development.local
frontend/.env.test.local
frontend/.env.production.local

frontend/npm-debug.log*
frontend/yarn-debug.log*
frontend/yarn-error.log*

# mobile

# dependencies
mobile/node_modules/

# Expo
mobile/.expo/
dist/
mobile/web-build/
mobile/%ProgramData%/

# Native
mobile/*.orig.*
mobile/*.jks
mobile/*.p8
mobile/*.p12
mobile/*.key
mobile/*.mobileprovision

# Metro
mobile/.metro-health-check*

# debug
mobile/npm-debug.*
mobile/yarn-debug.*
mobile/yarn-error.*

# macOS
mobile/.DS_Store
mobile/*.pem

# local env files
mobile/.env*.local

# typescript
mobile/*.tsbuildinfo

# server
# Ignore bundler config.
server/.bundle

# Ignore all environment files (except templates).
server/.env*
!server/.env*.erb

# Ignore all logfiles and tempfiles.
server/log/*
server/tmp/*
!server/log/.keep
!server/tmp/.keep

# Ignore pidfiles, but keep the directory.
server/tmp/pids/*
!server/tmp/pids/
!server/tmp/pids/.keep

# Ignore storage (uploaded files in development and any SQLite databases).
server/storage/*
!server/storage/.keep
server/tmp/storage/*
!server/tmp/storage/
!server/tmp/storage/.keep

# Ignore master key for decrypting credentials and more.
server/config/master.key
